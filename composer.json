{
    "name": "twilioman/twilio",
    "description": "Twilio API for Laravel",
    "type": "library",
    "keywords": ["sms","laravel","twilio"],
    "authors": [
        {
            "name": "chetan",
            "email": "chetandeep@singsys.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.5.0",
        "twilio/sdk":"5.*"
    },
    "autoload": {
        "psr-4": {
            "TwilioMan\\LaravelTwilio\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "TwilioMan\\LaravelTwilio\\Provider\\ServiceProvider"
            ],
            "aliases": {
                "Twilio": "TwilioMan\\LaravelTwilio\\Facade\\Facade"
            }
        }
    }
}
